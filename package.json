{
  "name": "webllm_engine_study",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:openwebui": "tsx openwebui-server.ts",
    "openwebui:up": "docker compose -f docker-compose.openwebui.yml up -d",
    "openwebui:down": "docker compose -f docker-compose.openwebui.yml down",
    "openwebui:logs": "docker compose -f docker-compose.openwebui.yml logs -f",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:deployed": "BASE_URL=$BASE_URL playwright test",
    "test:openwebui": "vitest run --config vitest.config.openwebui.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.80",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "dompurify": "^3.3.1",
    "express": "^4.18.2",
    "marked": "^17.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.2",
    "fake-indexeddb": "^6.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
